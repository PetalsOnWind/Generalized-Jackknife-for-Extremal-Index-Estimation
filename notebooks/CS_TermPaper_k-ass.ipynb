   "source": [
    "ns = [100, 200, 500, 1000, 2000, 5000, 10000]\n",
    "thetas = [0.2, 0.5, 0.8]\n",
    "\n",
    "for n in ns:\n",
    "    for theta in thetas:\n",
    "        k = ((2*(1-theta)*n*n)/(theta*(1+theta)*(1+theta)))**(1/3)\n",
    "        print(\"n = {0} and theta = {1}, k/n = {2:.4f}\".format(n, theta, k/n))"
   ]
